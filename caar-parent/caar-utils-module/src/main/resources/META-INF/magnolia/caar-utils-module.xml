			<implementation>es.arquia.magnolia.utils.ArchitectureFilesSupportArchitectImpl</implementation>
		</component>
		<component>
			<type>es.arquia.magnolia.utils.ArchitectureFilesSupportEvent</type>
			<implementation>es.arquia.magnolia.utils.ArchitectureFilesSupportEventImpl</implementation>
		</component>
		<component>
			<type>es.arquia.magnolia.utils.ArchitectureFilesSupportReview</type>
			<implementation>es.arquia.magnolia.utils.ArchitectureFilesSupportReviewImpl</implementation>
		</component>
		<component>
			<type>es.arquia.magnolia.utils.ArchitectureFilesSupportProject</type>
