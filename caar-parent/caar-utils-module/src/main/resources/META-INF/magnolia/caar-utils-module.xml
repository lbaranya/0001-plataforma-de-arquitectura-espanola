	<dependencies>
		<dependency>
			<name>core</name>
			<version>5.6/*</version>
		</dependency>
		<dependency>
			<name>ui-admincentral</name>
			<version>5.6/*</version>
		</dependency>
		<dependency>
			<name>mail</name>
			<version>5.5/*</version>
		</dependency>
		<dependency>
			<name>workflow-jbpm</name>
			<version>5.7/*</version>
		</dependency>
		<dependency>
			<name>dam-app</name>
			<version>2.3/*</version>
		</dependency>
		<dependency>
			<name>pages</name>
			<version>5.6/*</version>
		</dependency>
		<dependency>
			<name>cache</name>
			<version>5.6/*</version>
		</dependency>
		<!-- Add other dependencies here, e.g the mte (magnolia templating essentials). 
			<dependency> <name>mte</name> <version>0.7/*</version> </dependency> -->
